{
    "compilerOptions": {
      "target": "ES2020",
      "lib": ["ES2020", "DOM"],
      "module": "CommonJS",
      "moduleResolution": "Node",
      "types": ["@playwright/test", "node"],
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "outDir": "dist"
    },
    "include": ["src/**/*.ts", "playwright.config.ts"]
  }
  